{
  "name": "nextjs-ts-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "build:profile": "next build --profile",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build": "next build",
    "export": "yarn build && next export",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "upgrade-deps": "ncu -u -i",
    "pretty": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "yarn build && start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "yarn build && start-server-and-test start http://localhost:3000 cypress:headless",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.3.1",
    "@vanilla-extract/css": "^1.9.1",
    "@vanilla-extract/dynamic": "^2.0.2",
    "@vanilla-extract/recipes": "^0.2.5",
    "@vanilla-extract/sprinkles": "^1.5.0",
    "clsx": "^1.2.1",
    "next": "13.0.1",
    "next-seo": "^5.8.0",
    "next-sitemap": "^3.1.29",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use-refs": "^1.0.1",
    "use-debugger-hooks": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "18.11.4",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vanilla-extract/next-plugin": "^2.1.1",
    "cross-env": "^7.0.3",
    "cypress": "^10.10.0",
    "dotenv": "^16.0.3",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-check-updates": "^16.3.15",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  }
}
